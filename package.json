{
  "name": "vue-useproject",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix --cache",
    "format": "prettier --write src/",
    "// --- 新增的自动化脚本 ---": "",
    "init:mock": "cd mock-server && npm install",
    "dev:mock": "node mock-server/server.js",
    "start": "concurrently \"npm run dev\" \"npm run dev:mock\" --names \"FRONTEND,BACKEND\" --prefix-colors \"green,blue\" --kill-others"
  },
  "dependencies": {
    "axios": "^1.13.1",
    "echarts": "^6.0.0",
    "element-plus": "^2.11.7",
    "marked": "^17.0.1",
    "pinia": "^3.0.3",
    "vue": "^3.5.22",
    "vue-router": "^4.6.3",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.2",
    "@types/marked": "^5.0.2",
    "@types/node": "^22.18.11",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/tsconfig": "^0.8.1",
    "concurrently": "^9.2.1",
    "eslint": "^9.37.0",
    "eslint-plugin-vue": "~10.5.0",
    "jiti": "^2.6.1",
    "json-server": "^1.0.0-beta.3",
    "npm-run-all2": "^8.0.4",
    "prettier": "3.6.2",
    "typescript": "~5.9.0",
    "vite": "^7.1.11",
    "vite-plugin-mkcert": "^1.17.9",
    "vite-plugin-vue-devtools": "^8.0.3",
    "vue-tsc": "^3.1.1"
  }
}
